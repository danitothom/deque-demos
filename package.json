{
  "name": "deque-demos",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "server": "node server/index.js",
    "demo:html": "node scripts/run-demo1.js",
    "demo:react": "node scripts/run-demo2.js",
    "demo:accessible": "node scripts/run-demo3.js",
    "demo:linter": "node scripts/run-demo4.js",
    "demo:devtools": "node scripts/run-demo5.js",
    "demo:mobile": "node scripts/run-demo6.js",
    "demo:hub": "node scripts/run-demo7.js",
    "demo:workflow": "node scripts/run-demo8.js",
    "demo:all": "npm run demo:html && npm run demo:react && npm run demo:accessible && npm run demo:linter && npm run demo:devtools && npm run demo:mobile && npm run demo:hub && npm run demo:workflow",
    "scan": "node scripts/scan-file.js",
    "workshop": "npm run demo:all && npm run server",
    "deque:projects": "node scripts/list-deque-projects.js",
    "deque:verify": "node scripts/verify-deque-connection.js",
    "deque:setup": "npm run deque:verify",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:demo": "npm run cy:run -- --spec 'cypress/e2e/accessibility.cy.js'",
    "cy:real": "DEQUE_API_KEY=$DEQUE_API_KEY DEQUE_PROJECT_ID=$DEQUE_PROJECT_ID npm run cy:demo",
    "cy:demo:deque": "DEQUE_API_KEY=demo_key DEQUE_PROJECT_ID=demo_project npm run cy:demo",
    "test:a11y": "npm run cy:demo",
    "test:a11y:real": "npm run cy:real",
    "test:a11y:ci": "npm run cy:run -- --headless --spec 'cypress/e2e/accessibility.cy.js'",
    "cy:accessibility": "cypress run --spec \"cypress/e2e/accessibility/**/*.cy.js\"",
    "cy:accessibility:deque": "cypress run --env DEQUE_API_KEY=$DEQUE_API_KEY,DEQUE_PROJECT_ID=$DEQUE_PROJECT_ID --spec \"cypress/e2e/accessibility/**/*.cy.js\"",
    "cy:accessibility:demo": "DEQUE_API_KEY=demo_mode cypress run --spec \"cypress/e2e/accessibility/**/*.cy.js\"",
    "axe-watcher:init": "node scripts/init-axe-watcher.js",
    "axe-watcher:run": "node scripts/axe-watcher.js",
    "axe-watcher:monitor": "node scripts/axe-watcher.js --monitor",
    "axe-watcher:demo": "DEQUE_API_KEY=demo_mode node scripts/axe-watcher.js",
    "axe-watcher:config": "node scripts/axe-watcher.js --config",
    "axe-watcher:status": "node scripts/axe-watcher.js --status",
    "axe-watcher:report": "node scripts/generate-axe-report.js",
    "test:a11y:full": "npm run cy:accessibility && npm run axe-watcher:run",
    "test:a11y:full:deque": "npm run cy:accessibility:deque && npm run axe-watcher:run",
    "test:a11y:full:demo": "npm run cy:accessibility:demo && npm run axe-watcher:demo",
    "monitor:a11y": "npm run axe-watcher:monitor",
    "monitor:a11y:deque": "DEQUE_API_KEY=$DEQUE_API_KEY DEQUE_PROJECT_ID=$DEQUE_PROJECT_ID npm run axe-watcher:monitor",
    "deploy:a11y": "npm run test:a11y:full:deque",
    "ci:a11y": "npm run test:a11y:ci && npm run axe-watcher:run",
    "report:a11y": "npm run axe-watcher:report",
    "report:deque": "node scripts/generate-deque-report.js",
    "analyze:a11y": "npm run axe-watcher:run && npm run report:a11y",
    "clean:reports": "rm -rf reports/axe-watcher/*.json && rm -rf backups/axe-watcher/*.json",
    "backup:a11y": "node scripts/process-backups.js",
    "validate:setup": "npm run deque:verify && node scripts/validate-axe-setup.js"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.3",
    "axe-core": "^4.11.0",
    "cypress": "^15.6.0",
    "cypress-axe": "^1.7.0",
    "dotenv": "^16.6.1",
    "jsdom": "^22.1.0",
    "vite": "^4.3.9",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "keywords": [
    "accessibility",
    "a11y",
    "deque",
    "axe",
    "cypress",
    "testing",
    "wcag",
    "axe-watcher"
  ],
  "description": "Proyecto de demostración de herramientas Deque con integración Axe Watcher para monitoreo continuo de accesibilidad"
}